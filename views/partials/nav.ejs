  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <% if (user) { %>
      <img  class="profile-pic"
            src="<%= user.profile.avatar %>"
            alt="<%= user.profile.name %> avatar"
      />
      <a href="/auth/logout">
        <button type="submit">Logout</button>
      </a>
      <a href="/decks">
        <button type="submit">View Decks</button>
      </a>
      <a href="/decks/new">
        <button type="submit">Make a Deck</button>
      </a>

    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
  </nav>
